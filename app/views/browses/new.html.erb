
<%= form_with model: [ @browse] do |form| %>

  <div>
    <%= form.label :attachment %><br>
    <%= form.file_field :attachment %><br>
    <br>
  </div>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %><br>
    <br>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>



